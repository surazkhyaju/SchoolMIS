@model SchoolMISDayLog.Models.Developer
@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>
@if (ViewBag.message != null)
{
    <div class="alert alert-danger">
        @ViewBag.message
    </div>
}
@using (Html.BeginForm("Create", "Developer", FormMethod.Post, new { @class = "form-horizontal" }))
{

    <div class="form-group">
        <div>
            @Html.LabelFor(d => d.DeveloperName, new { @class = "col-md-3 control-label" })
        </div>
        <div class="col-md-9">
            @Html.TextBoxFor(d => d.DeveloperName, new { @class = "form-control" })
            @Html.ValidationMessageFor(d => d.DeveloperName)
        </div>
    </div>
    <div class="form-group">
        <div>
            @Html.LabelFor(d => d.Age, new { @class = "col-md-3 control-label" })
        </div>
        <div class="col-md-9">
            @Html.TextBoxFor(d => d.Age, new { @class = "form-control" })
            @Html.ValidationMessageFor(d => d.Age)

        </div>
    </div>
    <div class="form-group">
        <div>
            @Html.LabelFor(d => d.PhoneNumber, new { @class = "col-md-3 control-label" })
        </div>
        <div class="col-md-9">
            @Html.TextBoxFor(d => d.PhoneNumber, new { @class = "form-control" })
            @Html.ValidationMessageFor(d => d.PhoneNumber)
        </div>
    </div>
    <div class="form-actions">
        <div class="row">
            <div class="col-md-offset-3 col-md-9">
                <button type="submit" class="btn btn-primary">Submit</button>
                <button type="button" class="btn btn-default">Cancel</button>
            </div>
        </div>
    </div>



}




